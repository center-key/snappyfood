{
   "name": "snappy-food",
   "version": "0.0.1",
   "description": "Tastier than fast food, more convenient than sit-down",
   "license": "MIT",
   "type": "module",
   "main": "websites/www.snappyfood.org/index.html",
   "repository": "github:center-key/snappyfood",
   "homepage": "https://github.com/center-key/snappyfood",
   "bugs": "https://github.com/center-key/snappyfood/issues",
   "docs": "https://snappyfood.org",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "snappy",
      "food"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true
