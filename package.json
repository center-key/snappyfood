   "dependencies": {
      "dna-engine": "~3.0",
      "web-ignition": "~2.0"
   },
   "devDependencies": {
      "@fortawesome/fontawesome-free": "~6.4",
      "jshint": "~2.13",
      "w3c-html-validator": "~1.4"
