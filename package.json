   "scripts": {
      "pretest": "jshint . --exclude node_modules",
      "test": "w3c-html-validator"
   },
   "dependencies": {
      "dna.js": "~1.9",
      "jquery": "~3.6",
      "web-ignition": "~1.5"
   },
   "devDependencies": {
      "@fortawesome/fontawesome-free": "~6.0",
      "jshint": "~2.13",
      "w3c-html-validator": "~1.0"
   }
}
